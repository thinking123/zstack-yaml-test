{
	"include": [
		"./typings",
		"./src"
	],
	"exclude": [
		"./test",
		"./jest.confit.ts",
		"./ts-source",
	],
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"outDir": "out",
		"sourceMap": true,
		"noImplicitAny": false,
		"lib": [
			"ES2020"
		],
		"types": [
			"jest",
			"node"
		],
		"esModuleInterop": true,
		"downlevelIteration": true,
		"declaration": true,
	}
}