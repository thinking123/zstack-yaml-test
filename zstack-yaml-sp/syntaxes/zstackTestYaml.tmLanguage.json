{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "zstackTestYaml",
  "scopeName": "resource.injection",
  "injectionSelector": "source.yaml",
  "patterns": [
    {
      "include": "#resource"
    },
    {
      "include": "#keywords"
    }
  ],
  "repository": {
    "resource": {
      "patterns": [
        {
          "name": "support.class.zstackTestYaml",
          "match": "[A-Z][\\w]+"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.zstackTestYaml",
          "match": "\\b(if|while|for|return)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.zstackTestYaml",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.zstackTestYaml",
          "match": "\\\\."
        }
      ]
    }
  }
}
